# {{ ansible_managed }}

apply Service "web-homepage" {
    display_name =			"Website: freifunk-ingolstadt.de"
    vars.http_vhost =			host.vars.vhost
    check_command =			"http"
    vars.http_ssl =			"true"
    vars.http_uri =			"/neuigkeiten/"
    vars.http_string =			"Freifunk Ingolstadt - ein Projekt des"
    assign where host.address && "web-homepage" in host.vars.webpage
}
